{
  "name": "react-webpack-eslint-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate to setup webpack react and eslint",
  "main": "index.js",
  "scripts": {
    "dev": "cd webpack && BUILD_ENV=development webpack-dev-server --config webpack.dev.js",
    "qa": "cd webpack && BUILD_ENV=qa webpack-dev-server --config webpack.dev.js",
    "production": "cd webpack && BUILD_ENV=production webpack-dev-server --config webpack.dev.js",
    "build:dev": "cd webpack && BUILD_ENV=development webpack --progress --colors --config webpack.prod.js",
    "build:qa": "cd webpack && BUILD_ENV=qa webpack --progress --colors --config webpack.prod.js",
    "build:prod": "cd webpack && BUILD_ENV=production webpack --progress --colors --config webpack.prod.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
